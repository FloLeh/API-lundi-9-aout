<div class="center jumbotron">
  <h1>Voici les cryptomonnaies répertoriées</h1>
  <%= button_to "Mise à jour de la base de donnée", {action: 'maj'}, {class: "btn btn-primary"}  %>
  <% Crypto.all.each do |c| %>
